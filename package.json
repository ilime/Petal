{
  "name": "PetalDev",
  "version": "2.5.0",
  "description": "Douban.FM Client With Extra - - -",
  "main": "bundle/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron .",
    "dev:hot": "webpack-dev-server --config webpack/config.dev.js",
    "bundle:dev": "webpack --config webpack/config.dev.js --progress",
    "bundle:prod": "webpack --config webpack/config.prod.js --progress",
    "bundle:electron-dev": "cross-env NODE_ENV=development webpack --config webpack/config.electron.js --progress",
    "bundle:electron-prod": "cross-env NODE_ENV=production webpack --config webpack/config.electron.js --progress",
    "bundle:dev+electron-dev": "yarn run bundle:dev && yarn run bundle:electron-dev",
    "bundle:prod+elcetron-prod": "yarn run bundle:prod && yarn run bundle:electron-prod",
    "dist": "electron-builder --config electron-builder.json",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ilime/Petal.git"
  },
  "keywords": [
    "Douban.FM",
    "electron",
    "client"
  ],
  "author": "Yue Yang <g1enyy0ung@gmail.com> (http://g1eny0ung.site)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "@babel/preset-stage-2": "^7.0.0-beta.44",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^8.0.0-beta",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "electron-devtools-installer": "^2.2.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "react-hot-loader": "^4.0.1",
    "sass-loader": "^6.0.7",
